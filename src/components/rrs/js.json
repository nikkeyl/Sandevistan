{
	"Click Event Listener": {
		"prefix": "ck",
		"body": [
			"${1:document}.addEventListener('click', ${2:e} => {\n\t$3\n})"
		]
	},
	"Const": {
		"prefix": "dq",
		"body": [
			"const ${1:name} = document.querySelector('.${2:elem}')"
		]
	},
	"Constant With An Array Of Elements": {
		"prefix": "dqa",
		"body": [
			"const ${1:name} = document.querySelectorAll('.${2:elem}')"
		]
	},
	"Target Element": {
		"prefix": "te",
		"body": [
			"const targetElement = e.target"
		]
	},
	"Adding A Class": {
		"prefix": "ca",
		"body": [
			"${1:document}.classList.add('${2:class}')"
		]
	},
	"Toggling A Class": {
		"prefix": "ct",
		"body": [
			"${1:document}.classList.toggle('${2:class}')"
		]
	},
	"Removing A Class": {
		"prefix": "cr",
		"body": [
			"${1:document}.classList.remove('${2:class}')"
		]
	},
	"Get An Attribute": {
		"prefix": "ga",
		"body": [
			"${1:document}.getAttribute('${2:attr}')"
		]
	},
	"Set Attribute": {
		"prefix": "sa",
		"body": [
			"${1:document}.setAttribute('${2:attr}', ${3:value})"
		]
	},
	"Removing Attribute": {
		"prefix": "ra",
		"body": [
			"${1:document}.removeAttribute('${2:attr}')"
		]
	},
	"Inner HTML": {
		"prefix": "ih",
		"body": [
			"${1:document}.innerHTML = '${2:elem}'"
		]
	},
	"Text Content": {
		"prefix": "tc",
		"body": [
			"${1:document}.textContent = '${2:content}'"
		]
	},
	"Query Selector": {
		"prefix": "qs",
		"body": [
			"${1:document}.querySelector('${2:selector}')"
		]
	},
	"Query Selector All": {
		"prefix": "qsa",
		"body": [
			"${1:document}.querySelectorAll('${2:selector}')"
		]
	},
	"Event Listener": {
		"prefix": "ae",
		"body": [
			"${1:document}.addEventListener('${2:event}', ${3:e} => {\n\t$4\n})"
		]
	},
	"For Each": {
		"prefix": "fe",
		"body": [
			"${1:array}.forEach(${2:item} => {\n\t$3\n})"
		]
	},
	"Anonymous Function": {
		"prefix": "afn",
		"body": [
			"(${1:arguments}) => {\n\t$2\n}"
		]
	},
	"Function As A Property Of An Object": {
		"prefix": "ofn",
		"body": [
			"${1:functionName}: (${2:arguments}) => {\n\t$3\n}"
		]
	},
	"Set Interval": {
		"prefix": "si",
		"body": [
			"setInterval(($1) => {\n\t$2\n}, ${3:1000})"
		]
	},
	"Set Timeout": {
		"prefix": "st",
		"body": [
			"setTimeout(($1) => {\n\t$2\n}, ${3:1000})"
		]
	},
	"Json Parse": {
		"prefix": "jp",
		"body": [
			"JSON.parse(${0:object})"
		]
	},
	"Json Stringify": {
		"prefix": "js",
		"body": [
			"JSON.stringify(${0:object})"
		]
	},
	"Console Log": {
		"prefix": "cl",
		"body": [
			"console.log($0)"
		]
	},
	"Alert": {
		"prefix": "al",
		"body": [
			"alert('${0:message}')"
		]
	}
}
