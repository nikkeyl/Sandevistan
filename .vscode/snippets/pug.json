	"Active Class": {
		"prefix": "ac",
		"body": ".active"
	},
	"Image lazy": {
		"prefix": "imgl",
		"body": "img(data-src=\"@img$1\", alt=\"$2\")"
	},
	"Image": {
		"prefix": "img",
		"body": "img(src=\"@img$1\", alt=\"$2\")"
	},
	"IBG": {
		"prefix": "ibg",
		"body": ".${1:block}__image-ibg\n\timg(data-src=\"@img$2\", alt=\"${3:alt}\")"
	},
	"IBGA": {
		"prefix": "ibga",
		"body": "a.${1:block}__image-ibg(href=\"$2\")\n\timg(data-src=\"@img$3\", alt=\"${4:alt}\")"
	},
	"Logo": {
		"prefix": "logo",
		"body": "a.logo(href=\"index.html\")\n\timg(src=\"@img$1\", alt=\"${2:alt}\")"
	},
	"Sprite": {
		"prefix": "sprite",
		"body": "svg.${1:icon}\n\tuse(href=\"@img/icons/sprite.svg#${2:icon}\")"
	},
	"Button": {
		"prefix": "btn",
		"body": "button.button(type=\"${1:button}\") $2"
	},
	"Button Link": {
		"prefix": "abtn",
		"body": "a.button(href=\"$1\") $2"
	},
	"Menu": {
		"prefix": "menu",
		"body": "nav.menu\n\tul.menu__list\n\t\tli.menu__item\n\t\t\ta.menu__link(href=\"$1\") $2"
	},
	"Breadcrumbs": {
		"prefix": "bc",
		"body": "nav.breadcrumbs\n\tul.breadcrumbs__list\n\t\tli.breadcrumbs__item\n\t\t\ta.breadcrumbs__link(href=\"${1:index}.html\") ${2:prev}\n\t\tli.breadcrumbs__item\n\t\t\tspan.breadcrumbs__current ${3:current}"
	},
	"Change Font Range": {
		"prefix": "cfr",
		"body": ".${1:change-font}\n\tinput.${1:change-font}__range(id=\"range\", type=\"range\", value=\"${2:16}\", min=\"${3:16}\", max=\"${4:30}\", data-font-range)\n\tlabel.${1:change-font}__value(for=\"range\", data-font-value)"
	},
	"Change Font Buttons": {
		"prefix": "cfb",
		"body": ".${1:change-font}\n\tbutton.${1:change-font}__button.active(type=\"button\", data-font-size=\"${2:16}\") ${2:16}\n\tbutton.${1:change-font}__button.${1:change-font}__button--low(type=\"button\", data-font-size=\"${3:25}\") ${3:25}\n\tbutton.${1:change-font}__button.${1:change-font}__button--big(type=\"button\", data-font-size=\"${4:30}\") ${4:30}"
	},
	"Spollers": {
		"prefix": "spollers",
		"body": ".${1:spollers}(data-spollers=\"${3:768}\")\n\tdetails.${2:spoller}\n\t\tsummary.${2:spoller}__title(type=\"button\", data-spoller) ${5:title}\n\t\t.${2:spoller}__body ${6:content}"
	},
	"Tabs": {
		"prefix": "tabs",
		"body": ".${1:tabs}(data-tabs)\n\tnav.${1:tabs}__navigation(data-tabs-titles)\n\t\tbutton.${1:tabs}__title.tab-active(type=\"button\") ${2:btn}\n\t\tbutton.${1:tabs}__title(type=\"button\") ${3:btn}\n\t\tbutton.${1:tabs}__title(type=\"button\") ${4:btn}\n\t.${1:tabs}__content(data-tabs-body)\n\t\t.${1:tabs}__body\n\t\t\t${5:| content}\n\t\t.${1:tabs}__body\n\t\t\t${6:| content}\n\t\t.${1:tabs}__body\n\t\t\t${7:| content}"
	},
	"Show More": {
		"prefix": "showmore",
		"body": ".${1:showmore}(data-showmore)\n\t.${1:showmore}__content(data-showmore-content=\"${2:200}\") $3\n\tbutton.button(type=\"button\", data-showmore-button)\n\t\tspan ${4:Show more}\n\t\tspan ${5:Hide}"
	},
	"Swiper": {
		"prefix": "sf",
		"body": ".swiper\n\t.swiper-wrapper.${1:block}__wrapper\n\t\t.swiper-slide.${1:block}__slide\n\t\t\t$2\n\t.swiper-pagination\n\t\tbutton.swiper-button-prev(type=\"button\") $3\n\t\tbutton.swiper-button-next(type=\"button\") $4\n\t.swiper-scrollbar"
	},
	"Section": {
		"prefix": "sec",
		"body": "section.${1:block}\n\t.${1:block}__container\n\t\t$4"
	},
	"Section Response": {
		"prefix": "secres",
		"body": "section.${1:block}\n\t.${1:block}__inner\n\t\t.${1:block}__container\n\t\t\t.${1:block}__body\n\t\t\t\t$4\n\t.${1:block}__image\n\t\timg(data-src=\"@img$2\", alt=\"$3\")"
	},
	"Full Page Section": {
		"prefix": "fullpage",
		"body": ".${1:block}(data-fp)\n\t.${1:block}__section(data-fp-section)"
	},
	"Container": {
		"prefix": "cnt",
		"body": ".${1:block}__container\n\t$2"
	},
	"Rating": {
		"prefix": "rating",
		"body": ".rating.rating--set\n\t.rating__body\n\t\t.rating__active\n\t\t.rating__items\n\t\t\tinput.rating__item(type=\"radio\", value=\"1\", name=\"rating\")\n\t\t\tinput.rating__item(type=\"radio\", value=\"2\", name=\"rating\")\n\t\t\tinput.rating__item(type=\"radio\", value=\"3\", name=\"rating\")\n\t\t\tinput.rating__item(type=\"radio\", value=\"4\", name=\"rating\")\n\t\t\tinput.rating__item(type=\"radio\", value=\"5\", name=\"rating\")\n\t.rating__value 3.6"
	},
	"Form email": {
		"prefix": "formemail",
		"body": "form.${1:form}(method=\"${3:POST}\")\n\tinput.${1:form}__input(type=\"text\", placeholder=\"${4:E-mail}\", name=\"email\", data-required=\"email\")\n\tbutton.${1:form}__button.button(type=\"submit\") ${6:Send}"
	},
	"Quantity": {
		"prefix": "qa",
		"body": ".quantity(data-quantity)\n\tbutton.quantity__button.quantity__button--plus(type=\"button\", data-quantity-plus)\n\t.quantity__input\n\t\tinput(type=\"text\", value=\"1\", name=\"quantity\", data-quantity-value)\n\tbutton.quantity__button.quantity__button--minus(type=\"button\", data-quantity-minus)"
	},
	"Input": {
		"prefix": "inp",
		"body": "input.${1:input}(type=\"${2:text}\", name=\"${3:input}\", placeholder=\"$4\")"
	},
	"Textarea": {
		"prefix": "txta",
		"body": "textarea.${1:input}(name=\"${2:message}\", placeholder=\"$3\")"
	},
	"Select": {
		"prefix": "sel",
		"body": "select.${1:select}(name=\"select\")\n\toption(value=\"1\", selected) ${2:option}\n\toption(value=\"2\") ${3:option}\n\toption(value=\"3\") ${4:option}\n\toption(value=\"4\") ${5:option}"
	},
	"Radio Buttons": {
		"prefix": "opt",
		"body": ".options\n\tlabel.option\n\t\tspan.option__text ${1:text}\n\t\tinput(type=\"radio\", name=\"option\", checked)\n\t\tspan.option__box\n\tlabel.option\n\t\tspan.option__text ${2:text}\n\t\tinput(type=\"radio\", name=\"option\")\n\t\tspan.option__box"
	},
	"Checkbox": {
		"prefix": "chk",
		"body": "label.checkbox\n\tspan.checkbox__text ${1:text}\n\tinput(type=\"checkbox\", name=\"checkbox\")\n\tspan.checkbox__box\n\t\tsvg\n\t\t\tuse(href=\"@img/icons/sprite.svg#${2:check}\")"
	},
	"Add Popup Include": {
		"prefix": "pop",
		"body": "include pug/modules/popup"
	},
	"Add Preloader Include": {
		"prefix": "prl",
		"body": "include pug/modules/preloader"
	},
	"Add Pagination Include": {
		"prefix": "pag",
		"body": "include pug/modules/pagination"
	}
}
