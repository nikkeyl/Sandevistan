{
	"css.lint.duplicateProperties": "error",
	"css.hover.documentation": false,
	"css.hover.references": false,
	"[css]": {
		"editor.hover.enabled": false,
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
	},
	"editor.parameterHints.enabled": false,
	"explorer.confirmDragAndDrop": false,
	"editor.inlayHints.enabled": "off",
	"explorer.confirmDelete": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.hover.enabled": false,
	"editor.hover.sticky": false,
	"editor.cursorBlinking": "expand",
	"editor.cursorStyle": "line",
	"editor.cursorWidth": 2,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.formatOnPaste": true,
	"editor.linkedEditing": true,
	"editor.formatOnSave": true,
	"editor.hover.above": false,
	"files.autoSave": "afterDelay",
	"git.ignoreLimitWarning": true,
	"git.confirmSync": false,
	"html.format.extraLiners": "//head, //body, //html",
	"html.format.indentInnerHtml": true,
	"html.format.wrapLineLength": 500,
	"html.hover.documentation": false,
	"html.hover.references": false,
	"[html]": {
		"editor.foldingStrategy": "indentation"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"npm.fetchOnlinePackageInfo": false,
	"npm.scriptHover": false,
	"path-autocomplete.pathMappings": {
		"@scss": "${folder}/src/scss",
		"@img": "${folder}/src/img",
		"@js": "${folder}/src/js"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"scss.lint.duplicateProperties": "error",
	"scss.hover.documentation": false,
	"scss.hover.references": false,
	"scss.showErrors": true,
	"[scss]": {
		"editor.hover.enabled": false,
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
	},
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"typescript.format.enable": false,
	"workbench.colorTheme": "Synthwave x Fluoromachine",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.editor.untitled.hint": "hidden",
	"workbench.layoutControl.enabled": false,
	"workbench.editor.enablePreview": false,
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"editor.tabSize": 2,
	"scssFormatter.printWidth": 85,
	"scssFormatter.trailingComma": "none",
	"workbench.activityBar.visible": false,
	"breadcrumbs.enabled": false,
	"window.menuBarVisibility": "compact",
	"editor.smoothScrolling": true,
	"editor.minimap.enabled": false,
	"files.defaultLanguage": "html",
	"editor.detectIndentation": false,
	"editor.folding": false,
	"editor.glyphMargin": false,
	"editor.parameterHints.cycle": false,
	"editor.renderControlCharacters": false,
	"editor.renderWhitespace": "none",
	"security.workspace.trust.enabled": false,
	"editor.insertSpaces": false,
	"editor.fontFamily": "JetBrains Mono",
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"terminal.integrated.fontFamily": "JetBrains Mono",
	"terminal.integrated.fontSize": 13,
	"chat.editor.fontSize": 13,
	"markdown.preview.fontSize": 13,
	"debug.console.fontSize": 13
}
